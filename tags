DataBaseUtil	DataBaseUtil.class.php	/^class DataBaseUtil{$/;"	c
connect	DataBaseUtil.class.php	/^	public  function connect(){$/;"	f
db	callback.php	/^$db = new DataBaseUtil();$/;"	v
link	DataBaseUtil.class.php	/^		$this->link = mysql_connect('localhost', 'root', 'larry-andy') or die("数据库连接失败");$/;"	v
link	DataBaseUtil.class.php	/^	private static $link = null;$/;"	v
query	DataBaseUtil.class.php	/^	public function query($sql){$/;"	f
result	callback.php	/^$result = $db->query("select * from user;");$/;"	v
update	DataBaseUtil.class.php	/^	public function update($sql){$/;"	f
